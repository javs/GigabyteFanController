    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="LibreHardwareMonitor\Resources\inpout32.gz" />
    <None Remove="LibreHardwareMonitor\Resources\inpoutx64.gz" />
    <None Remove="LibreHardwareMonitor\Resources\WinRing0.gz" />
    <None Remove="LibreHardwareMonitor\Resources\WinRing0x64.gz" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="LibreHardwareMonitor\Resources\inpout32.gz" />
    <EmbeddedResource Include="LibreHardwareMonitor\Resources\inpoutx64.gz" />
    <EmbeddedResource Include="LibreHardwareMonitor\Resources\WinRing0.gz" />
